{
  "extends": "tslint:recommended",
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-reference": false,
    "ban-types": false,
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "private-static-method"
        ]
      }
    ],
    "no-console": [
      false
    ],
    "no-debugger": false
  }
}
